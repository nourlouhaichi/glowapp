{% block title %}Programme{% endblock %}

{% block body %}
    <h1>Programme</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ programme.id }}</td>
            </tr>
            <tr>
                <th>titre_pro</th>
                <td>{{ programme.titrePro }}</td>
            </tr>
            <tr>
                <th>Plan_pro</th>
                <td>{{ programme.planPro }}</td>
